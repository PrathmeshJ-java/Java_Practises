package LinkedList;

public class LinkedList {
	
	Node head;
	
	class Node{
		int data;
		Node next;
		
		Node(int data){
			this.data = data;
			this.next = null;
		}
		
		void addfirst(int data) {
			Node newNode = new Node(data);
			if(head==null) {
				head = newNode;
				return;
			}
			
			newNode.next = head;
			head = newNode;
			
		}
		
		void printlist() {
			Node current = head;
			while(current != null) {
				System.out.println(current+" ");
				current = current.next;
			}
		}
	}

}
