package LinkedList;

import java.util.spi.AbstractResourceBundleProvider;

public class LinkedList {
	
	Node head;
	
	class Node{
		int data;
		Node next;
		
		Node(int data){
			this.data = data;
			this.next = null;
		}
		
		void addfirst(int data) {
			Node newNode = new Node(data);)
			if(head==null) {
				head = new Node(data);
				return;
			}
			
			head.next = new Node(data);
			
			
			
		}
	}

}
